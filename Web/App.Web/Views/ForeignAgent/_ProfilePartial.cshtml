@model AddAddNewForeignCvRequest
<div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
    <div class="edit-profile-form profile-form">

        <div class="row">

            <div class="col-md-6">
                <div class="form-inner mb-25">
                    <label asp-for="cv.DateOfBirth">Date of Birth*</label>
                    <div class="input-area">
                        <img src="~/assets/images/icon/calender2.svg" alt="">
                        <input type="text" asp-for="cv.DateOfBirth" placeholder="03/08/1986" onchange="calculateAge()">
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-inner mb-25">
                    <label asp-for="cv.Age"></label>
                    <div class="input-area">
                        <img src="~/assets/images/icon/calender2.svg" alt="">
                        <input type="text" asp-for="cv.Age" readonly="readonly">
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-inner mb-25">
                    <label asp-for="cv.NationalityId"></label>
                    <div class="input-area">
                        <img src="~/assets/images/icon/nid.svg" alt="">
                        <select asp-for="cv.NationalityId" class="select1" asp-items="@ViewBag.Countries">
                            <option value="">Select Nationality </option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-inner mb-25">
                    <label asp-for="cv.PlaceOfBirthId"></label>
                    <div class="input-area">
                        <img src="~/assets/images/icon/home-2.svg" alt="">
                        <select asp-for="cv.PlaceOfBirthId" class="select1" asp-items="@ViewBag.Cities">
                            <option value="">Select Place of Birth </option>
                        </select>
                    </div>
                </div>
            </div>

            <div class="col-md-6">
                <div class="form-inner mb-25">
                    <label asp-for="cv.MartialStatusId"></label>
                    <div class="input-area">
                        <img src="~/assets/images/icon/marital-2.svg" alt="">
                        <select asp-for="cv.MartialStatusId" class="select1" asp-items="@ViewBag.martialStatus">
                            <option value="">Select Martial Status </option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-inner mb-25">
                    <label asp-for="cv.NoOfChildren"></label>
                    <div class="input-area">
                        <img src="~/assets/images/icon/hight.svg" alt="">
                        <input type="text" asp-for="cv.NoOfChildren" placeholder="Enter Number">
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-inner mb-25">
                    <label asp-for="cv.Gender">Gender</label>
                    <div class="input-area">
                        <img src="~/assets/images/icon/gender.svg " alt="">
                        <select asp-for="cv.Gender" class="select1">
                            <option value="">Select Gender</option>
                            <option value="0">@Gender.Male</option>
                            <option value="1">@Gender.Female</option>

                        </select>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-inner mb-25">
                    <label asp-for="cv.ReligionId"></label>
                    <div class="input-area">
                        <img src="~/assets/images/icon/religion.svg" alt="">
                        <select asp-for="cv.ReligionId" class="select1" asp-items="@ViewBag.Religion">
                            <option value="">Select Religion </option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-inner mb-25">
                    <label asp-for="cv.ContractPeriod"></label>
                    <div class="input-area">
                        <img src="~/assets/images/icon/hight.svg" alt="">
                        <input type="text" asp-for="cv.ContractPeriod" placeholder="Enter Number">
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-inner mb-25">
                    <label asp-for="cv.PassportNumber"></label>
                    <div class="input-area">
                        <img src="~/assets/images/icon/hight.svg" alt="">
                        <input type="text" asp-for="cv.PassportNumber" placeholder="Enter Number">
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-inner mb-25">
                    <label asp-for="cv.PlaceOfIssue"></label>
                    <div class="input-area">
                        <img src="~/assets/images/icon/hight.svg" alt="">
                        <input type="text" asp-for="cv.PlaceOfIssue" placeholder="Enter Number">
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-inner mb-25">
                    <label asp-for="cv.PassportDateOfIssue"></label>
                    <div class="input-area">
                        <img src="~/assets/images/icon/calender2.svg" alt="">
                        <input type="text" asp-for="cv.PassportDateOfIssue" placeholder="03/08/1986">
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-inner mb-25">
                    <label asp-for="cv.PassportDateOfExpiry"></label>
                    <div class="input-area">
                        <img src="~/assets/images/icon/calender2.svg" alt="">
                        <input type="text" asp-for="cv.PassportDateOfExpiry" placeholder="03/08/1986">
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-inner mb-25">
                    <label asp-for="cv.Height"></label>
                    <div class="input-area">
                        <img src="~/assets/images/icon/hight.svg" alt="">
                        <input type="text" asp-for="cv.Height" placeholder="5.6'">
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-inner mb-25">
                    <label asp-for="cv.Weight"></label>
                    <div class="input-area">
                        <img src="~/assets/images/icon/weight.svg" alt="">
                        <input asp-for="cv.Weight" type="text" placeholder="56">
                    </div>
                </div>
            </div>

            <div class="col-md-12">
                <div class="form-inner">
                    <button class="primry-btn-2 lg-btn w-unset" type="submit">Save Changes</button>
                </div>
            </div>

        </div>
    </div>
</div>